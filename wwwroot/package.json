{
  "name": "basic",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:client": "cross-env NODE_ENV=dev webpack --config ./build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=dev webpack --config ./build/webpack.server.config.js --progress --hide-modules",
    "watch:client": "yarn run build:client --watch",
    "watch:server": "yarn run build:server --watch",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "watch": "rimraf dist && concurrently  --raw \"npm run watch:client\" \"npm run watch:server\""
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "vue-loader": "^15.4.1",
    "vue-ssr-webpack-plugin": "^3.0.0",
    "vue-style-loader": "^4.1.2",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  }
}